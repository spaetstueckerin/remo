<h1>Listing cold_consumptions</h1>

<table>
  <tr>
    <th>Machinery</th>
    <th>Coldproduction</th>
    <th>Profitcapacity</th>
    <th>Requirementtype</th>
    <th>Colddistribution</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cold_consumptions.each do |cold_consumption| %>
  <tr>
    <td><%= cold_consumption.machinery_id %></td>
    <td><%= cold_consumption.coldProduction_id %></td>
    <td><%= cold_consumption.profitCapacity %></td>
    <td><%= cold_consumption.requirementType %></td>
    <td><%= cold_consumption.coldDistribution_id %></td>
    <td><%= link_to 'Show', cold_consumption %></td>
    <td><%= link_to 'Edit', edit_cold_consumption_path(cold_consumption) %></td>
    <td><%= link_to 'Destroy', cold_consumption, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

