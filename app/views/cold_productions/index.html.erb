<h1>Listing cold_productions</h1>

<table>
  <tr>
    <th>Manufacturer</th>
    <th>Buildtype</th>
    <th>Yearofmanufacturing</th>
    <th>Capacity</th>
    <th>Coolingcapacity</th>
    <th>Coolingmedium</th>
    <th>Rkwayofcooling</th>
    <th>Rkcoolingmedium</th>
    <th>Rkcapacity</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cold_productions.each do |cold_production| %>
  <tr>
    <td><%= cold_production.manufacturer %></td>
    <td><%= cold_production.buildType %></td>
    <td><%= cold_production.yearOfManufacturing %></td>
    <td><%= cold_production.capacity %></td>
    <td><%= cold_production.coolingCapacity %></td>
    <td><%= cold_production.coolingMedium %></td>
    <td><%= cold_production.rkWayOfCooling %></td>
    <td><%= cold_production.rkCoolingMedium %></td>
    <td><%= cold_production.rkCapacity %></td>
    <td><%= link_to 'Show', cold_production %></td>
    <td><%= link_to 'Edit', edit_cold_production_path(cold_production) %></td>
    <td><%= link_to 'Destroy', cold_production, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

