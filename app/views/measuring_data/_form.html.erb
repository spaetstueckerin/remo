<%= form_for(@measuring_datum, :multipart => true) do |f| %>
<table>
	<tr>
		<td><%= f.label :mnumber, "Messung" %>
		<td colspan=3><%= f.select :mnumber, Measuring.all.map{|m| [m.name, m.id]}%></td>
	</tr>
	<tr>
		<td><%= f.label :tsv_file, "Dateiupload" %></td>
		<td colspan=3><%= f.file_field :tsv_file %></td>
	</tr>
</table>
<br>
<%= f.submit "Upload >>", :class => "submit" %>

<% end %>



