html, body
{
    font-family: Verdana, 'Lucida Sans Unicode', sans-serif;
    font-size: 12px;
    color: #5e5e5e;
    padding: 10px;
    margin: 0 auto;
    margin-top: 20px
}

#header
{
    height: 80px;
    width: 1240px;
    background-color: #e1e2d4;
    padding: 20px;
}

#title
{
    float: left;
    line-height: 10px;
}

#logo
{
    position: absolute;
    height: auto;
    width: 200px;
    left: 1125px;
    top: 55px;
}

table
{
    page-break-inside: inherit;
    border-color: #555555;
    width: 750px;

}
th {
    background-color: #ECEDE1;
    margin: 5px;
    padding: 5px;

}

thead {
    background-color: #ECEDE1;
    margin: 10px;
    padding: 10px;
    text-align: center;
}

tbody{
    background-color:#F1F2EB;
    margin: 10px;
    padding: 10px;
    text-align: center;
}
canvas {
    page-break-after: always;
}
.data_table table
{
    page-break-before: always;
    align: center;
    border: 1px;

}
.zentriert
{
    text-align: center;
}
#data td{
    text-align: left;
}
#data td{
    text-align: left;
}
.canvas {

    width: 750px;
}